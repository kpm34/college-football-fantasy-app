{
  "name": "college-football-fantasy-app",
  "version": "1.0.0",
  "description": "Fantasy football platform for Power 4 conferences",
  "private": true,
  "scripts": {
    "schema:pull": "appwrite pull collections",
    "schema:push": "appwrite push collections --force",
    "schema:types": "appwrite types -l ts ./src/types/appwrite",
    "schema:sync": "npm run schema:pull && npm run schema:types",
    "schema:all": "npm run schema:push && npm run schema:types",
    "schema:list": "ts-node scripts/list-appwrite-collections.ts",
    "migrate:auth-users": "ts-node --transpile-only scripts/migrations/2025-08-15-backfill-auth-users.ts",
    "migrate:commissioner": "node --import 'data:text/javascript,import%20%7B%20register%20%7D%20from%20%22node:module%22;%20import%20%7B%20pathToFileURL%20%7D%20from%20%22node:url%22;%20register(%22ts-node/esm%22,%20pathToFileURL(%22./%22));' scripts/migrations/2025-08-14-backfill-commissioner.ts",
    "enhance:appwrite": "tsx scripts/appwrite-user-enhancements.ts",
    "dev": "next dev",
    "build": "SKIP_ENV_VALIDATION=true next build",
    "prebuild": "node scripts/generate-icons.js 2>/dev/null || true && npm run validate:env",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "lint:fix": "next lint --fix",
    "icons": "node scripts/generate-icons.js",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "preview": "vercel",
    "deploy": "vercel --prod",
    "clean": "rm -rf .next node_modules package-lock.json && npm install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:edge-config": "curl http://localhost:3001/api/edge-config",
    "validate:env": "echo 'Environment validation removed - handled by core/config/environment.ts'",
    "sync:rotowire": "node scripts/rotowire-sync-league.mjs",
    "seed-big12": "python3 scripts/seed_big12_draftboard.py 2>/dev/null || true",
    "collect-data": "python3 scripts/data_collector.py 2>/dev/null || true",
    "update-projections": "python3 scripts/projection_updater.py 2>/dev/null || true",
    "vercel-build": "next build",
    "predeploy": "npm run lint && npm run typecheck",
    "health": "curl -s https://cfbfantasy.app/api/health | jq .",
    "health:vercel": "vercel logs --follow",
    "ingest:ea": "ts-node scripts/ingestEA.ts --season=2025",
    "ingest:mockdraft": "ts-node scripts/ingestMockDraft.ts --season=2025",
    "ingest:depth": "ts-node scripts/ingestDepthCharts.ts --season=2025",
    "ingest:efficiency": "ts-node scripts/ingestTeamEfficiency.ts --season=2025",
    "reconcile:draftpool": "ts-node scripts/cleanup-draft-pool.ts --season=2025 --apply=true",
    "export:players": "ts-node scripts/export-college-players.ts --season=2025",
    "project:yearly": "ts-node functions/project-yearly-simple/index.ts --season=2025",
    "project:pro:weekly": "ts-node functions/project-pro-distributions/index.ts --season=2025 --week=1 --runs=5000 --seed=42",
    "project:pro:yearly": "ts-node functions/project-pro-distributions/index.ts --season=2025 --runs=5000 --seed=42",
    "recalc:custom": "ts-node functions/recalc-custom-projection/index.ts --season=2025 --user=USER_ID --player=PLAYER_ID",
    "check:versions": "ts-node scripts/check-file-versions.ts",
    "commit:safe": "npm run check:versions && git add -A && git commit"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.60.0",
    "@figma/code-connect": "^1.3.4",
    "@gsap/react": "^2.1.2",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@langchain/anthropic": "^0.3.26",
    "@modelcontextprotocol/sdk": "^1.17.2",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-navigation-menu": "^1.2.1",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-scroll-area": "^1.2.1",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@react-three/drei": "^10.6.1",
    "@react-three/fiber": "^9.3.0",
    "@react-three/postprocessing": "^3.0.4",
    "@sentry/nextjs": "^10.5.0",
    "@splinetool/react-spline": "^4.0.0",
    "@splinetool/runtime": "^1.9.32",
    "@studio-freight/lenis": "^1.0.42",
    "@vercel/analytics": "^1.5.0",
    "@vercel/kv": "^3.0.0",
    "@vercel/speed-insights": "^1.2.0",
    "ai": "^5.0.15",
    "appwrite": "^16.0.2",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv-cli": "^10.0.0",
    "figma-js": "^1.16.1-0",
    "framer-motion": "^11.18.2",
    "gsap": "^3.13.0",
    "jose": "^5.9.6",
    "langchain": "^0.3.30",
    "lenis": "^1.3.8",
    "leva": "^0.10.0",
    "lucide-react": "^0.454.0",
    "mcp-handler": "^1.0.1",
    "next": "15.0.3",
    "next-themes": "^0.4.4",
    "node-appwrite": "^14.1.0",
    "openai": "^5.12.2",
    "puppeteer": "^24.16.2",
    "react": "^19.0.0-rc-66855b96-20241106",
    "react-dom": "^19.0.0-rc-66855b96-20241106",
    "react-hook-form": "^7.54.0",
    "react-icons": "^5.5.0",
    "recharts": "^2.13.3",
    "sonner": "^1.7.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.179.1",
    "zod": "^3.25.76",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.117.0",
    "@types/node": "^20.19.10",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "autoprefixer": "^10.4.20",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.0.0",
    "eslint-config-next": "15.0.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "figma-transformer": "^2.1.0",
    "glob": "^11.0.3",
    "jest": "^29.7.0",
    "playwright": "^1.54.2",
    "postcss": "^8.4.49",
    "style-dictionary": "^4.4.0",
    "tailwindcss": "^3.4.16",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
