{
  "name": "college-football-fantasy-app",
  "version": "1.0.0",
  "description": "Fantasy football platform for Power 4 conferences",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "SKIP_ENV_VALIDATION=true next build",
    "prebuild": "node scripts/generate-icons.js 2>/dev/null || true && npm run validate:env",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "lint:fix": "next lint --fix",
    "icons": "node scripts/generate-icons.js",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "preview": "vercel",
    "deploy": "vercel --prod",
    "clean": "rm -rf .next node_modules package-lock.json && npm install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:edge-config": "curl http://localhost:3001/api/edge-config",
    "sync:appwrite": "tsx scripts/sync-appwrite-schema.ts",
    "sync:appwrite-platforms": "ts-node scripts/sync-appwrite-platforms.ts",
    "sync:env": "tsx scripts/sync-env-and-collections.ts",
    "sync:env:fix": "tsx scripts/sync-env-and-collections.ts --fix",
    "validate:env": "tsx scripts/validate-env-sync.ts",
    "sync:cfbd": "tsx scripts/sync-cfbd-data.ts",
    "sync:rotowire": "node scripts/rotowire-sync-league.mjs",
    "seed-big12": "python3 scripts/seed_big12_draftboard.py 2>/dev/null || true",
    "collect-data": "python3 scripts/data_collector.py 2>/dev/null || true",
    "update-projections": "python3 scripts/projection_updater.py 2>/dev/null || true",
    "vercel-build": "next build",
    "postinstall": "npm run sync:appwrite 2>/dev/null || true",
    "db:sync": "node scripts/sync-appwrite-complete.js",
    "db:check": "node scripts/check-all-permissions.js",
    "predev": "npm run db:check",
    "db:fix-rosters": "node scripts/fix-rosters-permissions.js",
    "db:fix-players": "node scripts/fix-players-permissions.js",
    "predev": "npm run db:check",
    "predeploy": "npm run db:check && npm run lint && npm run typecheck",
    "verify:sync": "node -r dotenv/config scripts/verify-sync.js dotenv_config_path=.env.local",
    "verify:env": "node scripts/check-env.js",
    "setup:storage": "node -r dotenv/config scripts/setup-appwrite-storage.js dotenv_config_path=.env.local",
    "health": "curl -s https://cfbfantasy.app/api/health | jq .",
    "health:vercel": "vercel logs --follow",
    "test:commissioner": "node -r dotenv/config scripts/test-commissioner-settings.js dotenv_config_path=.env.local",
    "update:check": "node scripts/update-tracker.js check",
    "update:summary": "node scripts/update-tracker.js summary",
    "update:dataflow": "node scripts/update-tracker.js dataflow",
    "update:track": "node scripts/update-tracker.js track",
    "setup:rotowire": "node scripts/setup-rotowire-credentials.js",
    "sync:cfbd": "node -r dotenv/config scripts/sync-cfbd-data.js dotenv_config_path=.env.local"
  },
  "dependencies": {
    "@gsap/react": "^2.1.2",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@modelcontextprotocol/sdk": "^1.17.2",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-navigation-menu": "^1.2.1",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-scroll-area": "^1.2.1",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@react-three/drei": "^10.6.1",
    "@react-three/fiber": "^9.3.0",
    "@react-three/postprocessing": "^3.0.4",
    "@sentry/nextjs": "^10.5.0",
    "@splinetool/react-spline": "^4.0.0",
    "@splinetool/runtime": "^1.9.32",
    "@studio-freight/lenis": "^1.0.42",
    "@vercel/analytics": "^1.5.0",
    "@vercel/kv": "^3.0.0",
    "@vercel/speed-insights": "^1.2.0",
    "appwrite": "^16.0.2",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.18.2",
    "gsap": "^3.13.0",
    "jose": "^5.9.6",
    "lenis": "^1.3.8",
    "leva": "^0.10.0",
    "lucide-react": "^0.454.0",
    "mcp-handler": "^1.0.1",
    "next": "15.0.3",
    "next-themes": "^0.4.4",
    "node-appwrite": "^14.1.0",
    "react": "^19.0.0-rc-66855b96-20241106",
    "react-dom": "^19.0.0-rc-66855b96-20241106",
    "react-hook-form": "^7.54.0",
    "react-icons": "^5.5.0",
    "recharts": "^2.13.3",
    "sonner": "^1.7.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.179.1",
    "zod": "^3.23.8",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "autoprefixer": "^10.4.20",
    "dotenv": "^17.2.1",
    "eslint": "^9.0.0",
    "eslint-config-next": "15.0.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "glob": "^11.0.3",
    "jest": "^29.7.0",
    "playwright": "^1.54.2",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
