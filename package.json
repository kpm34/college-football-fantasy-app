{
  "name": "college-football-fantasy-app",
  "version": "1.0.0",
  "description": "Fantasy football platform for Power 4 conferences",
  "private": true,
  "scripts": {
    "schema:pull": "appwrite pull collections",
    "schema:push": "appwrite push collections --force",
    "schema:types": "appwrite types -l ts ./src/types/appwrite",
    "schema:sync": "npm run schema:pull && npm run schema:types",
    "schema:sync": "npx tsx ops/common/scripts/sync-appwrite-simple.ts",
    "schema:all": "npm run schema:push && npm run schema:types",
    "schema:list": "ts-node ops/common/scripts/list-appwrite-collections.ts",
    "schema:validate": "npx tsx ops/common/scripts/validate-schema-compliance.ts",

    "seed": "npx tsx ops/common/scripts/seed.ts",
    "migrate:auth-users": "ts-node --transpile-only scripts/migrations/2025-08-15-backfill-auth-users.ts",
    "migrate:commissioner": "node --import 'data:text/javascript,import%20%7B%20register%20%7D%20from%20%22node:module%22;%20import%20%7B%20pathToFileURL%20%7D%20from%20%22node:url%22;%20register(%22ts-node/esm%22,%20pathToFileURL(%22./%22));' scripts/migrations/2025-08-14-backfill-commissioner.ts",
    "enhance:appwrite": "tsx ops/common/scripts/appwrite-user-enhancements.ts",
    "eval_proj": "npx tsx ops/common/scripts/eval_proj.ts",
    "dev": "next dev",
    "build": "SKIP_ENV_VALIDATION=true next build",
    "prebuild": "npx tsx ops/common/guards/forbid-test-pages.ts && npx tsx ops/common/guards/forbid-legacy-collections.ts && node ops/common/scripts/generate-icons.js 2>/dev/null || true && npm run validate:env",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit -p tsconfig.typecheck.json",
    "lint:fix": "next lint --fix",
    "icons": "node ops/common/scripts/generate-icons.js",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "preview": "vercel",
    "deploy": "vercel --prod",
    "clean": "rm -rf .next node_modules package-lock.json && npm install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:schema": "jest tests/schema.contract.test.ts",
    "audit-errors": "npx tsx ops/common/scripts/audit-projection-errors.ts",
    "mock:ensure": "npx tsx ops/common/scripts/appwrite/ensure-mock-draft-schema.ts",
    "mock:run": "npx tsx ops/common/scripts/mock-draft/run.ts",
    "mock:concurrency": "npx tsx ops/common/scripts/mock-draft/concurrency-test.ts",
    "mock:concurrency:simple": "npx tsx ops/common/scripts/mock-draft/concurrency-test-simple.ts",
    "mock:human:e2e": "npx tsx ops/common/scripts/mock-draft/human-e2e.ts",
    "mock:human:e2e:10": "NUM_TEAMS=10 ROUNDS=12 npx tsx ops/common/scripts/mock-draft/human-e2e.ts",
    "mock:human:e2e:12": "NUM_TEAMS=12 ROUNDS=10 npx tsx ops/common/scripts/mock-draft/human-e2e.ts",
    "mock:human:e2e:14": "NUM_TEAMS=14 ROUNDS=8 npx tsx ops/common/scripts/mock-draft/human-e2e.ts",
    "mock:human:e2e:24": "NUM_TEAMS=24 ROUNDS=5 TIMER_SEC=1 npx tsx ops/common/scripts/mock-draft/human-e2e.ts",
    "test:schema:watch": "jest tests/schema.contract.test.ts --watch",
    "test:infrastructure": "jest tests/infrastructure.contract.test.ts",
    "test:contracts": "jest tests/*.contract.test.ts",
    "test:smoke": "playwright test --config=tests/smoke/playwright.config.ts",
    "test:smoke:ci": "playwright test --config=tests/smoke/playwright.config.ts --reporter=json",
    "test:e2e": "node ops/common/scripts/test-e2e.js",
    "test:e2e:prod": "node ops/common/scripts/test-e2e.js https://cfbfantasy.app",
    "test:e2e:local": "node ops/common/scripts/test-e2e.js http://localhost:3000",
    "test:edge-config": "curl http://localhost:3001/api/edge-config",
    "validate:env": "echo 'Environment validation removed - handled by core/config/environment.ts'",
    "sync:rotowire": "node ops/common/scripts/rotowire-sync-league.mjs",
    "seed-big12": "python3 ops/common/scripts/seed_big12_draftboard.py 2>/dev/null || true",
    "collect-data": "python3 ops/common/scripts/data_collector.py 2>/dev/null || true",
    "update-projections": "python3 ops/common/scripts/projection_updater.py 2>/dev/null || true",
    "vercel-build": "next build",
    "predeploy": "npm run lint && npm run typecheck",
    "health": "curl -s https://cfbfantasy.app/api/health | jq .",
    "health:vercel": "vercel logs --follow",
    "ingest:ea": "ts-node ops/common/scripts/ingestEA.ts --season=2025",
    "ingest:mockdraft": "ts-node ops/common/scripts/ingestMockDraft.ts --season=2025",
    "ingest:depth": "ts-node ops/common/scripts/ingestDepthCharts.ts --season=2025",
    "ingest:efficiency": "ts-node ops/common/scripts/ingestTeamEfficiency.ts --season=2025",
    "reconcile:draftpool": "ts-node ops/common/scripts/cleanup-draft-pool.ts --season=2025 --apply=true",
    "export:players": "ts-node ops/common/scripts/export-college-players.ts --season=2025",
    "export:players:json": "npx tsx ops/common/scripts/export-college-players-json.ts",
    "export:database": "npx tsx ops/common/scripts/export-complete-database.ts",
    "compress:prompt": "npx tsx ops/common/scripts/compress-players-for-prompt.ts",

    
    
    
    
    "check:versions": "ts-node ops/common/scripts/check-file-versions.ts",
    "commit:safe": "npm run check:versions && git add -A && git commit",
    "generate:types": "node schema/generators/types.ts",
    "generate:appwrite": "node schema/generators/appwrite.ts",
    "generate:env": "node schema/generators/env.ts",
    "generate:docs": "npx tsx ops/common/scripts/generate-schema-docs.ts",
    "generate:all": "npm run generate:types && npm run generate:appwrite && npm run generate:env && npm run generate:docs",
    "seed:appwrite": "node schema/generators/seed-appwrite.ts",
    "cleanup:schema": "node ops/common/scripts/cleanup-old-schema-references.js",
    "restore:user-leagues": "tsx ops/common/scripts/restore-user-league-relationships.ts",
    "validate:schema": "tsx ops/common/scripts/validate-schema-compliance.ts",
    "validate:ssot": "npx tsx ops/common/guards/validate-ssot-integrity.ts",
    "detect:drift": "npx tsx ops/common/guards/detect-schema-drift.ts",
    "guard:all": "npm run validate:ssot && npm run detect:drift && npm run validate:schema",
    "diff:ssot": "npx tsx ops/common/guards/diff-ssot-vs-appwrite.ts",
    "env:pull": "vercel env pull .env.local",
    "guards:ssot": "tsx ops/common/guards/validate-ssot-integrity.ts",
    "guards:drift": "tsx ops/common/guards/detect-schema-drift.ts",
    "ops:cursor:context": "tsx ops/common/scripts/generate-cursor-context.ts",
    "ops:codemod": "tsx ops/common/codemods/run-codemod.ts",
    "fx:deploy": "appwrite functions deploy --all",
    "fx:test": "tsx functions/appwrite/test-local.ts",
    "test:ssot": "npx tsx ops/common/scripts/test-ssot-workflow.ts",
    "setup:monitoring": "tsx ops/common/scripts/setup-monitoring.ts",
    "monitoring:dashboard": "curl -s https://cfbfantasy.app/api/monitoring/dashboard | jq .",
    "demo:glob": "npx tsx ops/common/scripts/demo-enhanced-glob.ts",
    "demo:glob:basic": "npx tsx ops/common/scripts/demo-enhanced-glob.ts --basic",
    "ingest:run": "npx tsx ops/common/scripts/run-data-ingestion.ts"
  },
