{
  "projectId": "college-football-fantasy-app",
  "projectName": "College Football Fantasy App",
  "functions": [
    {
      "$id": "trade-processor",
      "name": "Trade Processor",
      "runtime": "node-18.0",
      "execute": ["any"],
      "events": [
        "databases.college-football-fantasy.collections.trades.documents.*.create",
        "databases.college-football-fantasy.collections.trades.documents.*.update"
      ],
      "schedule": "",
      "timeout": 30,
      "enabled": true,
      "logging": true,
      "entrypoint": "index.js",
      "commands": "npm install",
      "ignore": [
        "node_modules",
        ".npm"
      ],
      "path": "."
    }
  ]
}
