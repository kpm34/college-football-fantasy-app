{
  "projectId": "college-football-fantasy-app",
  "projectName": "College Football Fantasy App",
  "functions": [
    {
      "$id": "draft-reminder",
      "name": "Draft Reminder",
      "runtime": "node-18.0",
      "execute": ["any"],
      "events": [],
      "schedule": "0 * * * *",
      "timeout": 30,
      "enabled": true,
      "logging": true,
      "entrypoint": "index.js",
      "commands": "npm install",
      "ignore": [
        "node_modules",
        ".npm"
      ],
      "path": "."
    }
  ]
}
