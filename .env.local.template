# Appwrite API key with full access
APPWRITE_API_KEY=REPLACE_ME # Example: your-api-key-here

# Appwrite endpoint URL
APPWRITE_ENDPOINT=https://nyc.cloud.appwrite.io/v1

# Appwrite project ID
APPWRITE_PROJECT_ID=college-football-fantasy-app

# Appwrite database ID
DATABASE_ID=college-football-fantasy

# Public Appwrite endpoint
NEXT_PUBLIC_APPWRITE_ENDPOINT=https://nyc.cloud.appwrite.io/v1

# Public Appwrite project ID
NEXT_PUBLIC_APPWRITE_PROJECT_ID=college-football-fantasy-app

# Public Appwrite database ID
NEXT_PUBLIC_APPWRITE_DATABASE_ID=college-football-fantasy

# Leagues collection ID
NEXT_PUBLIC_APPWRITE_COLLECTION_LEAGUES=leagues

# Rosters collection ID
NEXT_PUBLIC_APPWRITE_COLLECTION_ROSTERS=rosters

# Players collection ID
NEXT_PUBLIC_APPWRITE_COLLECTION_PLAYERS=college_players

# Draft picks collection ID
NEXT_PUBLIC_APPWRITE_COLLECTION_DRAFT_PICKS=draft_picks

# Lineups collection ID
NEXT_PUBLIC_APPWRITE_COLLECTION_LINEUPS=lineups

# Player projections collection ID
NEXT_PUBLIC_APPWRITE_COLLECTION_PLAYER_PROJECTIONS=player_projections

# Yearly projections collection ID
NEXT_PUBLIC_APPWRITE_COLLECTION_PROJECTIONS_YEARLY=projections_yearly

# Weekly projections collection ID
NEXT_PUBLIC_APPWRITE_COLLECTION_PROJECTIONS_WEEKLY=projections_weekly

# Model inputs collection ID
NEXT_PUBLIC_APPWRITE_COLLECTION_MODEL_INPUTS=model_inputs

# User custom projections collection ID
NEXT_PUBLIC_APPWRITE_COLLECTION_USER_CUSTOM_PROJECTIONS=user_custom_projections

# Activity log collection ID
NEXT_PUBLIC_APPWRITE_COLLECTION_ACTIVITY_LOG=activity_log

# College Football Data API key
CFBD_API_KEY=REPLACE_ME # Example: your-cfbd-key

# Secret for cron job authentication
CRON_SECRET=REPLACE_ME # Example: generate-random-string

