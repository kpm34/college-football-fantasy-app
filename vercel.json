{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "crons": [
    {
      "path": "/api/cron/data-sync",
      "schedule": "0 */6 * * *"
    },
    {
      "path": "/api/cron/weekly-scoring",
      "schedule": "0 3 * * 1"
    },
    {
      "path": "/api/cron/poll-jobs",
      "schedule": "*/5 * * * *"
    }
  ]
}
