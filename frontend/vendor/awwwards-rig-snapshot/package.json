{
  "name": "awwwards-rig",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "preview": "vercel",
    "deploy": "vercel --prod",
    "prepare": "husky",
    "analyze": "ANALYZE=true next build",
    "clean": "rm -rf .next node_modules package-lock.json && npm install",
    "setup:edge-config": "./scripts/setup-edge-config.sh",
    "test:edge-config": "curl http://localhost:3000/api/edge-config"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.1",
    "@ai-sdk/gateway": "^1.0.4",
    "@ai-sdk/openai": "^2.0.7",
    "@react-three/drei": "^10.6.1",
    "@react-three/fiber": "^9.3.0",
    "@react-three/postprocessing": "^3.0.4",
    "@types/gsap": "^1.20.2",
    "@vercel/edge-config": "1.4.0",
    "@vercel/kv": "^3.0.0",
    "ai": "^3.1.25",
    "framer-motion": "^12.23.12",
    "gsap": "^3.13.0",
    "lenis": "^1.3.8",
    "leva": "^0.10.0",
    "next": "15.4.6",
    "r3f-perf": "^7.2.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "three": "^0.179.1",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.4.6",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "critters": "^0.0.23",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "husky": "latest",
    "lint-staged": "latest",
    "prettier": "^3.4.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix --max-warnings 0",
      "prettier --write"
    ],
    "*.{json,md}": "prettier --write"
  },
  "engines": {
    "node": ">=18.17 <23"
  }
}
