    "@heroicons/react": "^2.2.0",
    "@modelcontextprotocol/sdk": "^1.17.2",
    "@react-three/drei": "^10.6.1",
    "@react-three/fiber": "^9.3.0",
    "@react-three/postprocessing": "^3.0.4",
    "@splinetool/react-spline": "^4.1.0",
    "@splinetool/runtime": "^0.9.0",
    "@types/three": "^0.178.1",
    "@vercel/analytics": "^1.5.0",
    "@vercel/edge-config": "^1.4.0",
    "@vercel/kv": "^3.0.0",
    "ai": "^5.0.0",
    "animejs": "^4.1.2",
    "appwrite": "^18.2.0",
    "chalk": "^5.5.0",
    "cheerio": "^1.1.2",
    "chrome-ai": "^1.11.1",
    "commander": "^14.0.0",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.23.12",
    "gsap": "^3.13.0",
    "lenis": "^1.3.8",
    "leva": "^0.10.0",
    "mcp-handler": "^1.0.1",
    "next": "15.4.5",
    "node-appwrite": "^17.2.0",
    "ora": "^8.2.0",
    "playwright": "^1.54.2",
    "r3f-perf": "^7.2.3",
    "react": "^18",
    "react-dom": "^18",
    "three": "^0.179.1",
    "zod": "^3.25.76",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@types/animejs": "^3.1.13",
    "@types/gsap": "^1.20.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.21",
    "eslint": "^8",
    "eslint-config-next": "15.4.5",
    "husky": "latest",
    "lint-staged": "latest",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "5.9.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix --max-warnings 0",
      "prettier --write"
    ],
    "*.{json,md}": "prettier --write"
  },
  "engines": {
    "node": ">=18.17 <23"
  }
}
